/* 细线边框 */
.b-t-e,
.b-b-e {
	position: relative;
}
.b-t-e:before {
	content: '';
	position: absolute;
	border-top: 1px solid #eee;
	-webkit-transform: scaleY(0.5);
	transform: scaleY(0.5);
	top: 0;
	left: 0;
	right: 0;
}
.b-b-e:after {
	content: '';
	position: absolute;
	border-bottom: 1px solid #eee;
	-webkit-transform: scaleY(0.5);
	transform: scaleY(0.5);
	bottom: 0;
	left: 0;
	right: 0;
}
.b-r-f {
	border-right: 1rpx solid #fff;
}

/* 右边一半边框 */
.b-r-d {
	position: relative;
}
.b-r-d::after {
	content: '';
	position: absolute;
	width: 1px;
	background: #C0C0C0;
	height: 100%;
	-webkit-transform: scale(0.5, 0.5);
	transform: scale(0.5, 0.5);
	top: 0%;
	right: 0;
}
.b-r-d:last-child::after {
	display: none;
}

