	background: #C2C2C2;
	position: relative;
	width: 50rpx;
	height: 50rpx;
	border-radius: 50%;
	padding: 0;
	margin: 0;
}
.cartadd::after {
	position: absolute;
	left: 50%;
	top: 50%;
	-webkit-transform: translate(-50%, -50%);
	        transform: translate(-50%, -50%);
	content: "";
	height: 5rpx;
	width: 20rpx;
	background: #fff;
	display: block;
	border-radius: 10rpx;
}
