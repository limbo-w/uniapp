/*+-*/
.reducecon {
	height: 60rpx;
	overflow: hidden;
	transition: all 0.3s linear;
	opacity: 0;
	/* width:0; */
	-webkit-transform: rotate(180deg);
	        transform: rotate(180deg);
	-webkit-transform: translateX(50rpx);
	        transform: translateX(50rpx);
}
.reducecon.active {
	opacity: 1;
	min-width: 105rpx;
	-webkit-transform: rotate(0);
	        transform: rotate(0);
}
