.weui-cells {
	position: relative;
	margin-top: 30rpx;
	background-color: #FFFFFF;
	line-height: 1.41176471;
	font-size: 28rpx;
	color: #333;
}
.weui-cells:before {
	position: absolute;
	content: '';
	top: 0;
	left: 0;
	right: 0;
	height: 1px;
	-webkit-transform: scaleY(0.5);
	        transform: scaleY(0.5);
	background-color: #eee;
}
.weui-cells:after {
	position: absolute;
	content: '';
	bottom: 0;
	left: 0;
	right: 0;
	height: 1px;
	-webkit-transform: scaleY(0.5);
	        transform: scaleY(0.5);
	background-color: #eee;
}
.weui-cells__title {
	margin-top: .77em;
	margin-bottom: .3em;
	padding-left: 15px;
	padding-right: 15px;
	color: #999999;
	font-size: 28rpx;
}
.weui-cells_after-title {
	margin-top: 0;
}
.weui-cells__tips {
	margin-top: .3em;
	color: #999999;
	padding-left: 15px;
	padding-right: 15px;
	font-size: 28rpx;
}
.weui-cell {
	background: #FFF;
	padding: 30rpx;
	position: relative;
	display: flex;
	align-items: center;
}
.weui-cell:after {
	content: '';
	position: absolute;
	border-bottom: 1px solid #eee;
	-webkit-transform: scaleY(0.5);
	transform: scaleY(0.5);
	bottom: 0;
	right: 0;
	left: 30rpx;
	/* content: '';
	display: block;
	position: absolute;
	width: 100%;
	height: 1px;
	background: #eee;
	transform: scaleY(0.5);
	left: 30rpx;
	bottom: 0;
	right: auto;
	top: auto; */
}
.isright:after {
	right: 30rpx;
}
.weui-cell:last-child:after {
	display: none;
}
.noafter:after {
	display: none;
}
.weui-cell_active {
	background-color: #ECECEC;
}
.weui-cell__bd {
	flex: 1;
}
.weui-cell__ft {
	text-align: right;
	color: #999999;
}
.weui-check__label:active {
	background-color: #ECECEC;
}
.weui-check {
	position: absolute;
	left: -9999px;
}
.weui-cell__ft_in-radio {
	padding-left: 0.35em;
}
.weui-label {
	width: 170rpx;
	font-size: 28rpx;
	word-wrap: break-word;
	word-break: break-all;
	color: #333333;
	min-width: 130rpx;
}
.weui-input {
	font-size: 28rpx;
	height: 2.28823529em;
	min-height: 2.28823529em;
	line-height: 2.28823529em;
	color: #333333;
}
.weui-agree__link {
	margin-left: 10rpx;
	display: inline;
	color: #586C94;
}
.weui-loading {
	margin: 0 5px;
	width: 20px;
	height: 20px;
	display: inline-block;
	vertical-align: middle;
	-webkit-animation: weuiLoading 1s steps(12, end) infinite;
	animation: weuiLoading 1s steps(12, end) infinite;
	background: transparent url(data:image/svg+xml;base64,) no-repeat;
	background-size: 100%;
}
@-webkit-keyframes weuiLoading {
0% {
		-webkit-transform: rotate3d(0, 0, 1, 0deg);
		transform: rotate3d(0, 0, 1, 0deg);
}
100% {
		-webkit-transform: rotate3d(0, 0, 1, 360deg);
		transform: rotate3d(0, 0, 1, 360deg);
}
}
@keyframes weuiLoading {
0% {
		-webkit-transform: rotate3d(0, 0, 1, 0deg);
		transform: rotate3d(0, 0, 1, 0deg);
}
100% {
		-webkit-transform: rotate3d(0, 0, 1, 360deg);
		transform: rotate3d(0, 0, 1, 360deg);
}
}
.weui-loadmore {
	margin: 0 auto;
	/* width: 65%; */
	padding: 30rpx auto;
	font-size: 28rpx;
	text-align: center;
}
.weui-loadmore__tips {
	display: inline-block;
	vertical-align: middle;
}

